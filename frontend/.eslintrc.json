{
  "env": {
      "es2021": true
  },
  "extends": [
      "airbnb",
      "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "no-console": 0
  }
}